#!/bin/sh
#SBATCH --nodes=1
#SBATCH --ntasks-per-node=1
#SBATCH --array=1-500
#SBATCH --mem=10gb
#SBATCH --time=05:00:00
#SBATCH --job-name=sas_logistic
#SBATCH --error=./logs/slurm-output/simulate.%j.%a.err
#SBATCH --output=./logs/slurm-output/simulate.%j.%a.out

module load sas/9.4

mkdir -p "./simulations/${SLURM_ARRAY_TASK_ID}"
cp ../bin/logistic.sas "./simulations/${SLURM_ARRAY_TASK_ID}"
cd "./simulations/${SLURM_ARRAY_TASK_ID}"
sas -nodms logistic.sas

rm -rf logistic.sas
